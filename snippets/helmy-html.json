    "HTML - Estructura Básica HTML5": {
        "prefix": "_HTML5",
        "body": [
            "<!DOCTYPE html>",
            "<html lang=\"es\">",
            "",
            "<head>",
            "    <meta charset=\"UTF-8\">",
            "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
            "    <title>${1:Título}</title>",
            "</head>",
            "",
            "<body>",
            "    $2",
            "</body>",
            "",
            "</html>"
        ],
        "description": "HTML - Estructura Básica HTML5"
    },
