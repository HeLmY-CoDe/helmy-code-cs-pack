    "PHP - Open tag": {
        "prefix": "_php_open_tag",
        "body": [
            "<?php",
            "$0"
        ],
        "description": "PHP - Open tag"
    },
    "PHP-HTML - insert PHP code": {
        "prefix": "_php_tags",
        "body": [
            "<?php",
            "",
            "$1",
            "",
            "?>"
        ],
        "description": "PHP-HTML - insert PHP code"
    },
    "PHP-HTML - echo $var": {
        "prefix": "_php_echo_var",
        "body": "<?php echo \\$${1:var}; ?>",
        "description": "PHP-HTML - echo $var"
    },
    "PHP-HTML - short echo tags": {
        "prefix": "_php_short_echo_tags",
        "body": "<?= $1; ?>",
        "description": "PHP-HTML - short echo tags"
    },
    "PHP-HTML - echo isset ternary": {
        "prefix": "_php_echo_isset_ternary",
        "body": "<?php echo isset(${1:cond}) ? ${2:true} : ${3:false}; ?>",
        "description": "PHP-HTML - echo isset ternary"
    },
    "PHP - Alternative 'if' syntax": {
        "prefix": "_php_alternative_if",
        "body": [
            "<?php if (${1:cond}) : ?>",
            "",
            "    ${2:code}",
            "",
            "<?php endif; ?>"
        ],
        "description": "PHP - Alternative 'if' syntax"
    },
    "PHP - Alternative 'while' syntax": {
        "prefix": "_php_alternative_while",
        "body": [
            "<?php while (${1:cond}) : ?>",
            "",
            "    ${2:code}",
            "",
            "<?php endwhile; ?>"
        ],
        "description": "PHP - Alternative 'while' syntax"
    },
    "PHP - Alternative 'for' syntax": {
        "prefix": "_php_alternative_for",
        "body": [
            "<?php for (\\$${1:i} ${2:=} ${3:0}; \\$${1:i} ${4:<} ${5:\\$n}; \\$${1:i}${6:++}) : ?>",
            "",
            "    ${7:code}",
            "",
            "<?php endfor; ?>"
        ],
        "description": "PHP - Alternative 'for' syntax"
    },
    "PHP - Alternative 'foreach' syntax": {
        "prefix": "_php_alternative_foreach",
        "body": [
            "<?php foreach (\\$${1:list} as \\$${2:item}) : ?>",
            "",
            "    ${3:code}",
            "",
            "<?php endforeach; ?>"
        ],
        "description": "PHP - Alternative 'foreach' syntax"
    },
    "PHP - Alternative 'foreach - key' syntax": {
        "prefix": "_php_alternative_foreach_key",
        "body": [
            "<?php foreach (\\$${1:list} as \\$${2:key} => \\$${3:value}) : ?>",
            "",
            "    ${4:code}",
            "",
            "<?php endforeach; ?>"
        ],
        "description": "PHP - Alternative 'foreach - key' syntax"
    },
    "HTML - Section Comment": {
        "prefix": "_html_section_comment",
        "body": [
            "<!----------------------------------------------------------------",
            "# ${1:Comment}",
            "---------------------------------------------------------------->"
        ],
        "description": "HTML - Section Comment"
    },
    "HTML - ES MODULES script tags": {
        "prefix": "_html_es_modules_script_tags",
        "body": "<script src=\"${1:./path/file.js}\" type=\"module\"></script>",
        "description": "HTML - ES MODULES script tags"
    },
    "HTML - ES MODULES script tags - NO SUPPORT BROWSERS": {
        "prefix": "_html_es_modules_script_tags_NO_SUPPORT_BROWSERS",
        "body": "<script src=\"${1:./path/file.js}\" nomodule></script>",
        "description": "HTML - ES MODULES script tags - NO SUPPORT BROWSERS"
    }
