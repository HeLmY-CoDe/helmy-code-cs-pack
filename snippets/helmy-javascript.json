{
    "JS - Alert": {
        "prefix": "_js_alert",
        "body": [
            "alert(${1:foo});"
        ],
        "description": "JS - Alert"
    },
    "JS - console.log": {
        "prefix": "_js_console-log",
        "body": [
            "console.log(${1:foo});"
        ],
        "description": "JS - console.log"
    },
    "JS - Text console.log": {
        "prefix": "_js_text-console-log",
        "body": [
            "console.log('${1:foo}:', ${1:foo});"
        ],
        "description": "JS - Text console.log"
    },
    "JS - console.log - return": {
        "prefix": "_js_console-log-return",
        "body": [
            "console.log(${1:foo});",
            "return;"
        ],
        "description": "JS - console.log - return"
    },
    "JS - console.log template literals": {
        "prefix": "_js_console-log-template-literals",
        "body": [
            "console.log(`${1:foo}`);"
        ],
        "description": "JS - console.log template literals"
    },
    "JS - Variable en template literals": {
        "prefix": "_js_$",
        "body": "\\${${1:foo}}",
        "description": "JS - Variable en template literals"
    },
    "JQuery - $.ajax": {
        "prefix": "_jq_ajax",
        "body": [
            "$.ajax({\r",
            "\turl: '${1:ajax/}',\r",
            "\ttype: '${2:POST}',\r",
            "\tdataType: '${3:json}',\r",
            "\tcontentType: false,\r",
            "\tprocessData: false,\r",
            "\tcache: false,\r",
            "\tdata: ${4:data},\r",
            "\tbeforeSend: function () {\r",
            "\t\t$5",
            "\t}\r",
            "\r",
            "}).done(function (res) {\r",
            "\t$6",
            "}).fail(function (err) {\r",
            "\t$7",
            "}).always(function () {\r",
            "\t$8",
            "})"
        ],
        "description": "JQuery - $.ajax"
    },
    "JQuery - ready()": {
        "prefix": "_jq_ready",
        "body": [
            "$(document).ready(function() {",
            "    ${1:console.log('READY!');}",
            "});"
        ],
        "description": "JQuery - ready()"
    },
    "JQuery - ready() shorthand": {
        "prefix": "_jq_ready-shorthand",
        "body": [
            "$(function () {\r",
            "    ${1:console.log('READY!');}",
            "});"
        ],
        "description": "JQuery - ready() shorthand"
    },
    "JS - Comment Separator": {
        "prefix": "_js_comment-separator",
        "body": [
            "/* ------------------------- */"
        ],
        "description": "JS - Comment Separator"
    },
    "JS - Comment Separator Text": {
        "prefix": "_js_text-comment-separator",
        "body": [
            "/* ------- ${1:Text} ------- */"
        ],
        "description": "JS - Comment Separator Text"
    },
    "JS - Section Comment": {
        "prefix": "_js_section-comment",
        "body": [
            "/*--------------------------------------------------------------",
            "# ${1:Comment}",
            "--------------------------------------------------------------*/"
        ],
        "description": "JS - Section Comment"
    }
}