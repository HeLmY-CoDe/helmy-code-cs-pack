{
    "JS - DomContentLoaded Listener (DOM READY!)": {
        "prefix": "_js_DomContentLoaded",
        "body": [
            "document.addEventListener('DOMContentLoaded', function() {",
            "    ${1:console.log('READY!');}",
            "});"
        ],
        "description": "JS - DomContentLoaded Listener (DOM READY!)"
    },
    "JS - Alert": {
        "prefix": "_js_alert",
        "body": [
            "alert(${1:foo});"
        ],
        "description": "JS - Alert"
    },
    "JS - console.log": {
        "prefix": "_js_console_log",
        "body": [
            "console.log(${1:foo});"
        ],
        "description": "JS - console.log"
    },
    "JS - Text console.log": {
        "prefix": "_js_text_console_log",
        "body": [
            "console.log('${1:foo}:', ${1:foo});"
        ],
        "description": "JS - Text console.log"
    },
    "JS - console.log - return": {
        "prefix": "_js_console_log_return",
        "body": [
            "console.log(${1:foo});",
            "return;"
        ],
        "description": "JS - console.log - return"
    },
    "JS - console.log template literals": {
        "prefix": "_js_console_log_template_literals",
        "body": [
            "console.log(`${1:foo}`);"
        ],
        "description": "JS - console.log template literals"
    },
    "JS - Variable en template literals": {
        "prefix": "_js_$",
        "body": "\\${${1:foo}}",
        "description": "JS - Variable en template literals"
    },
    "JS - Event Prevent Default": {
        "prefix": "_js_event.preventDefault",
        "body": "${1:event}.preventDefault();",
        "description": "JS - Event Prevent Default"
    },
    "JS - Arrow function": {
        "prefix": "_js_arrow_function",
        "body": [
            "${1:const} ${2:functionName} = ${3:async} (${4:params}) => {",
            "    $5",
            "}"
        ],
        "description": "JS - Arrow function"
    },
    "JQuery - Selector": {
        "prefix": "_jq_selector",
        "body": "\\$('${1:#}${2:element}')",
        "description": "JQuery - Selector"
    },
    "JQuery - $.ajax": {
        "prefix": "_jq_ajax",
        "body": [
            "$.ajax({\r",
            "\turl: '${1:ajax/}',\r",
            "\ttype: '${2:POST}',\r",
            "\tdataType: '${3:json}',\r",
            "\tcontentType: false,\r",
            "\tprocessData: false,\r",
            "\tcache: false,\r",
            "\tdata: ${4:data},\r",
            "\tbeforeSend: function () {\r",
            "\t\t$5",
            "\t}\r",
            "\r",
            "}).done(function (res) {\r",
            "\t$6",
            "}).fail(function (err) {\r",
            "\t$7",
            "}).always(function () {\r",
            "\t$8",
            "})"
        ],
        "description": "JQuery - $.ajax"
    },
    "JQuery - ready()": {
        "prefix": "_jq_ready",
        "body": [
            "$(document).ready(function() {",
            "    ${1:console.log('READY!');}",
            "});"
        ],
        "description": "JQuery - ready()"
    },
    "JQuery - ready() shorthand": {
        "prefix": "_jq_ready_shorthand",
        "body": [
            "$(function () {\r",
            "    ${1:console.log('READY!');}",
            "});"
        ],
        "description": "JQuery - ready() shorthand"
    },
    "JQuery - ready() arrow shorthand": {
        "prefix": "_jq_ready_arrow_shorthand",
        "body": [
            "$(() => {\r",
            "    ${1:console.log('READY!');}",
            "})"
        ],
        "description": "JQuery - ready() arrow shorthand"
    },
    "JS - Comment Separator": {
        "prefix": "_js_comment_separator",
        "body": [
            "/* ------------------------- */"
        ],
        "description": "JS - Comment Separator"
    },
    "JS - Comment Separator Text": {
        "prefix": "_js_text_comment_separator",
        "body": [
            "/* ------- ${1:Text} ------- */"
        ],
        "description": "JS - Comment Separator Text"
    },
    "JS - Section Comment": {
        "prefix": "_js_section_comment",
        "body": [
            "/*--------------------------------------------------------------",
            "# ${1:Comment}",
            "--------------------------------------------------------------*/"
        ],
        "description": "JS - Section Comment"
    }
}